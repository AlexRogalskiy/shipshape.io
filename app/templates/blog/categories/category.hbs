<HeroBlock @heroClasses="blog" @icon="landing">
  <h1>{{category}}</h1>
</HeroBlock>

<div class="blog-posts section flex flex-wrap justify-center">
  <div class="section-content">
    <BlogPostMenu
      @navigatePages={{action "navigatePages"}}
      @page={{this.page}}
      @posts={{this.model}}
      @totalPosts={{this.model.length}}
    >
    </BlogPostMenu>
  </div>
</div>