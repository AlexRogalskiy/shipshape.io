{{#if isAtLeastDesktop}}
  <a data-scroll aria-hidden="true" href="{{href}}">
    <div class="mouse-icon">
      <div class="wheelContainer">
        <div class="wheel"></div>
        <div class="wheelSecond"></div>
      </div>
    </div>
  </a>
{{else}}
  <a data-scroll aria-hidden="true" href="{{href}}">
    <div class="mobile-scroll">
      {{svg-jar "mobile-scroll" height="32px" width="32px"}}
    </div>
  </a>
{{/if}}